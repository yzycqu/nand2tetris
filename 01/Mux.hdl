/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = o1);
    And(a = a, b = o1, out = o2);
    And(a = b, b = sel, out = o3);
    Or(a = o2, b = o3, out = out);
}